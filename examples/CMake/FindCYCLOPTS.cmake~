
#SET(CYCLOPTS_ROOT_DIR "" CACHE PATH "CYCLOPTS root directory")
set(CYCLOPTS_ROOT_DIR $ENV{CYCLOPTS_ROOT_DIR})

FIND_PATH(CYCLOPTS_INCLUDE_DIR CycloptsUtilsConfig.h
  HINTS ${CYCLOPTS_ROOT_DIR}/include/cyclopts
)
FIND_LIBRARY(CYCLOPTS_CBC_LIBRARY
  NAMES Cbc libCbc
  HINTS ${CYCLOPTS_ROOT_DIR}/lib/cyclopts
  HINTS ${CYCLOPTS_ROOT_DIR}/lib
)